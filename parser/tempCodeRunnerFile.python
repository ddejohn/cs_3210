def mat_split(mat, col):
    el_mat = []
    r_mat = []
    for row in mat:
        el_mat.append(row[:col])
        r_mat.append(row[col:])
    return el_mat, r_mat


a = [[f"{i}{j}" for j in range(10)] for i in range(10)]

el, r = mat_split(a, 4)

print("LEFT")
for row in el:
    print(row,)

print("RIGHT")
for row in r:
    print(row,)
